@{
    Layout="Shared/_Layout.cshtml";
}

<h1>Band Tracker</h1>
<h2>Venue: @Model["venue"].GetName()</h2>
<hr />

@if (@Model["venueBands"].Count != 0)
{
    <p>No venues have been added yet!</p>
}
else
{
    <h4>Here are all the bands in this venue:</h4>
    <ul>
    @foreach (var band in @Model["venueBands"])
    {
        <li>@band.GetName()</li>
    }
    </ul>

}




<h4><a href="/">Return home</a></h4>
